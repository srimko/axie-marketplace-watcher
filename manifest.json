{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/axie-marketplace-watcher16.png",
      "32": "/images/axie-marketplace-watcher32.png",
      "48": "/images/axie-marketplace-watcher48.png",
      "128": "/images/axie-marketplace-watcher128.png"
    }
  },
  "icons": {
    "16": "/images/axie-marketplace-watcher16.png",
    "32": "/images/axie-marketplace-watcher32.png",
    "48": "/images/axie-marketplace-watcher48.png",
    "128": "/images/axie-marketplace-watcher128.png"
  }
}
